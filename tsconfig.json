{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "es2017"],
    "noEmit": false,
    "module": "commonjs",
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*.ts", "next-env.d.ts"],
  "exclude": ["src/web/*.*"]
}
